<h1>Index Page for Recipies </h1>
<a href="/recipes/new">Create New Recipe</a>


<% if @recipes != nil
      @recipes.all.each_with_index(1) do |recipe|
        %>
        Recipe Name : <h3><%= recipe.name %></h3></br>
        <a href="/recipes/<%=recipe.id %>">Show Page</a></br>
      <% end %>
<% end %>
